{
  "name": "@aptosis/aptos-client",
  "version": "0.0.1",
  "description": "Lightweight Aptos API client.",
  "author": "Ian Macalinao <ian@aptos.is>",
  "homepage": "https://github.com/aptosis/aptos-api#readme",
  "license": "Apache-2.0",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "src/",
    "dist/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aptosis/aptos-api.git"
  },
  "scripts": {
    "build": "tsc && tsc --project tsconfig.cjs.json"
  },
  "bugs": {
    "url": "https://github.com/aptosis/aptos-api/issues"
  },
  "dependencies": {
    "@aptosis/aptos-api": "workspace:^",
    "@vespaiach/axios-fetch-adapter": "^0.3.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@saberhq/tsconfig": "^2.0.2",
    "axios": "^0.27.2",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "axios": "^0.27.2"
  }
}
