{
  "name": "@aptosis/aptos-typed-api",
  "version": "0.1.1",
  "description": "Stricter Aptos API types.",
  "author": "Ian Macalinao <ian@aptos.is>",
  "homepage": "https://github.com/aptosis/aptos-api#readme",
  "license": "Apache-2.0",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "src/",
    "dist/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aptosis/aptos-api.git"
  },
  "scripts": {
    "build": "tsc && tsc --project tsconfig.cjs.json"
  },
  "bugs": {
    "url": "https://github.com/aptosis/aptos-api/issues"
  },
  "dependencies": {
    "@aptosis/aptos-api": "workspace:^",
    "@movingco/move-types": "^0.4.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@saberhq/tsconfig": "^2.0.3",
    "jsbi": "^4.3.0",
    "typescript": "^4.7.4"
  },
  "typedoc": {
    "readmeFile": "./README.md"
  },
  "peerDependencies": {
    "axios": "^0.27"
  }
}
